{
  "name": "express-director",
  "version": "0.5.0",
  "description": "A file system based router for express that eases standardizing controller handling",
  "scripts": {
    "build": "tsup-node src/index.ts --format cjs,esm --dts --clean",
    "lint": "eslint 'src/**/*.[tj]s' 'test/**/*.[tj]s'",
    "lint:fix": "eslint --fix . 'src/**/*.[tj]s' 'test/**/*.[tj]s'",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "install:all": "npm i && npm --prefix test/cjs-app install && npm --prefix test/mjs-app install && npm --prefix test/ts-app install",
    "test:cjs": "npm --prefix test/cjs-app run test",
    "test:mjs": "npm --prefix test/mjs-app run test",
    "test:ts": "npm --prefix test/ts-app run test",
    "test": "npm run build && npm run install:all && npm run lint && npm run test:mjs && npm run test:cjs && npm run test:ts"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.21",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "prettier": "^2.5.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "ajv": "~8.9",
    "express": "~4.17",
    "swagger-ui-express": "~4.3"
  },
  "optionalDependencies": {
    "@parcel/config-default": "^2.3.2",
    "@parcel/core": "^2.3.2",
    "handlebars": "^4.7.7"
  }
}
