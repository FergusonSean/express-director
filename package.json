{
  "name": "express-director",
  "version": "0.1.0",
  "description": "A file system based router for express that eases standardizing controller handling",
  "scripts": {
    "build": "tsup-node src/index.ts --format cjs,esm --dts --clean",
    "lint": "eslint 'src/**/*.[tj]s' 'test/**/*.[tj]s'",
    "lint:fix": "eslint --fix . 'src/**/*.js' 'test/**/*.js'",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "test:cjs": "npm --prefix test/cjs-app install && npm --prefix test/cjs-app run test",
    "test:mjs": "npm --prefix test/mjs-app install && npm --prefix test/mjs-app run test",
    "test": "npm run build && npm run test:mjs && npm run test:cjs"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.21",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "ajv": "~8.9",
    "express": "~4.5"
  }
}
